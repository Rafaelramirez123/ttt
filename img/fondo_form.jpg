<script>
document.addEventListener("DOMContentLoaded", function () {
const menuItems = document.querySelectorAll(".megamenu_navbar__content");
const menuIcon = document.querySelector(".mobile-menu");
const navbar = document.querySelector(".megamenu_navbar__div");
const headeroverlay = document.createElement("div");
const headerNav = document.querySelector("#topsrchSec");
headeroverlay.classList.add("headeroverlay");
headeroverlay.style.display = "none";
document.body.appendChild(headeroverlay);
// Show overlay function
function showOverlay() {
headeroverlay.style.display = "block";
document.body.classList.add("topnav-no-scroll");
}
// Hide overlay function
function hideOverlay() {
headeroverlay.style.display = "none";
document.body.classList.remove("topnav-no-scroll");
}
// Check if screen width is below 1024px
function isMobileScreen() {
return window.innerWidth < 1024;
}
// Toggle menu icon and navbar change classes
menuIcon.addEventListener("click", (event) => {
event.stopPropagation(); // Prevent the click from bubbling up
const isOpen = menuIcon.classList.toggle("open");
navbar.classList.toggle("change");
if (isOpen) {
headerNav.classList.add("header_nav");
showOverlay();
} else {
headerNav.classList.remove("header_nav");
document.querySelectorAll(".megamenu_dropdown").forEach((dropdown) => {
dropdown.classList.remove("active");
dropdown.classList.remove("courselist");
});
navbar.classList.remove("hidden");
document
.querySelectorAll(".megamenu_navbar__contents")
.forEach((item) => {
item.classList.remove("hidden-courses");
});
document
.querySelectorAll(".mobile_dropdown_coursesuni")
.forEach((dropdown) => {
dropdown.classList.remove("subactive");
});
hideOverlay();
}
});
menuItems.forEach((item) => {
item.addEventListener("click", function (event) {
document.querySelectorAll(".megamenu_navbar__content").forEach((item) => {
item.classList.remove("m_active");
});
document
.querySelectorAll(".megamenu_navbar__contents")
.forEach((item) => {
item.classList.add("hidden-courses");
item.classList.remove('parent-scroll');
});
document.querySelectorAll(".megamenu_dropdown").forEach((dropdown) => {
if (dropdown !== item.querySelector(".megamenu_dropdown")) {
dropdown.classList.remove("active");
}
});
// Toggle the clicked megamenu
const dropdown = item.querySelector(".megamenu_dropdown");
if (dropdown) {
// const dropdownClassList = Array.from(dropdown.classList);
const isActive = dropdown.classList.toggle("active");
item.classList.toggle("m_active");
if (isMobileScreen()) {
dropdown.classList.add("active");
}
// Show or hide overlay based on dropdown state
if (isActive) {
item.classList.add("m_active");
if (!isMobileScreen()) {
showOverlay();
}
if($("#findCourses").hasClass("m_active")){
topNavViewClickstream('Courses & Uni','TopNav');
topNavClickGALogging('courses_and_uni_click');
} else if($("#exploreDest").hasClass("m_active")){
topNavViewClickstream('Explore destinations','TopNav');
topNavClickGALogging('explore_destinations_click');
} else if($("#costsFinance").hasClass("m_active")){
topNavViewClickstream('Costs & Finance','TopNav');
topNavClickGALogging('costs_and_finance_click');
} if($("#howToApply").hasClass("m_active")){
topNavViewClickstream('How to apply?','TopNav');
topNavClickGALogging('how_to_apply_click');
} if($("#movingAbroad").hasClass("m_active")){
topNavViewClickstream('Moving abroad','TopNav');
topNavClickGALogging('moving_abroad_click');
} 
} else {
item.classList.remove("m_active");
if (!isMobileScreen()) {
hideOverlay();
}
}
}
event.stopPropagation();
});
});
// Close megamenu when clicking outside
document.addEventListener("click", () => {
navbar.classList.remove("change");
menuIcon.classList.remove("open");
document.querySelectorAll(".megamenu_dropdown").forEach((dropdown) => {
dropdown.classList.remove("active");
});
document.querySelectorAll(".megamenu_navbar__content").forEach((item) => {
item.classList.remove("m_active");
});
document.querySelectorAll(".megamenu_dropdown").forEach((dropdown) => {
dropdown.classList.remove("active");
dropdown.classList.remove("courselist");
});
headerNav.classList.remove("header_nav");
document
.querySelectorAll(".mobile_dropdown_coursesuni")
.forEach((dropdown) => {
dropdown.classList.remove("subactive");
});
document.querySelectorAll(".megamenu_navbar__contents").forEach((item) => {
item.classList.remove("hidden-courses");
});
hideOverlay();
});
// Prevent closing when clicking inside the megamenu
document.querySelectorAll(".megamenu_dropdown").forEach((dropdown) => {
dropdown.addEventListener("click", (event) => {
event.stopPropagation();
});
});
document
.querySelector(".megamenu_navbar__div")
.addEventListener("click", (event) => {
event.stopPropagation();
});
// Hide overlay when clicking on it
headeroverlay.addEventListener("click", () => {
document.querySelectorAll(".megamenu_dropdown").forEach((dropdown) => {
dropdown.classList.remove("active");
});
hideOverlay();
});
});
document.addEventListener("DOMContentLoaded", () => {
const headlines = document.querySelectorAll(".dropmenu_coursesuni .dropmenu_headline");
headlines.forEach((headline) => {
headline.addEventListener("click", function () {
document
.querySelectorAll(".mobile_dropdown_courses.courselist")
.forEach((dropdown) => {
dropdown.classList.remove("courselist");
});
document
.querySelectorAll(".mobile_dropdown_coursesuni")
.forEach((dropdown) => {
dropdown.classList.add("subactive");
});
// Toggle the corresponding dropdown
const dropdown = this.parentElement.querySelector(
".mobile_dropdown_courses"
);
if (dropdown) {
dropdown.classList.toggle("courselist");
}
});
});
});
// Add event listener to mobile_dropdown_heading to go back to megamenu_navbar__content
document.querySelectorAll(".mobile_dropdown_heading").forEach((heading) => {
heading.addEventListener("click", function (event) {
document
.querySelectorAll(".mobile_dropdown_coursesuni")
.forEach((dropdown) => {
dropdown.classList.remove("subactive");
});
});
});
document.querySelectorAll(".mobile_megamenu_heading").forEach((heading) => {
heading.addEventListener("click", function (event) {
document.querySelectorAll(".megamenu_navbar__contents").forEach((item) => {
item.classList.remove("hidden-courses");
item.classList.add('parent-scroll');
});
});
});
</script>
