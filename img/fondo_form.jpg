flex-direction: column;
width: 24px;
min-height: 24px;
padding: 0px 1px;
gap: 5px;
justify-content: center;
align-items: center;
}
.hamburgerline {
display: flex;
width: 22px;
height: 2.3px;
background-color: #000000;
transform-origin: left;
}
.open .hamburgerline {
width: 18px;
height: 2px;
border-radius: 20px;
}
.open .hamburgerline1 {
opacity: 0;
}
.open .hamburgerline2 {
position: absolute;
transform: rotate(-44deg) translate(-3px, 6px);
}
.open .hamburgericon {
background-color: #fff;
border-radius: 50px;
width: 32px;
height: 32px;
align-items: center;
justify-content: center;
max-height: 32px;
}
.open .hamburgerline3 {
position: absolute;
transform: rotate(44deg) translate(-3px, -6px);
}
.mobile-menu {
display: flex;
justify-content: center;
transition: none;
padding: 18px 16px;
max-height: 59px;
z-index: 1020;
}
.mobile-menu:hover {
background-color: #e5e7eb;
}
.mobile-menu.open:hover {
background-color: rgba(0, 0, 0, 0);
}
.mobile-menu:not(:active) {
background-color: #ffffff;
}
.mobile-menu.open:not(:active) {
background-color: rgba(0, 0, 0, 0);
}
.mobile-menu.open {
transform: translateX(342px);
padding: 0;
position: fixed;
top: 16px;
}
.md_d-flex {
display: flex;
}
header.bgwht {
z-index: 1006!important;
}
